{
  "name": "reactmap",
  "version": "1.24.1",
  "private": true,
  "description": "React based frontend map.",
  "license": "MIT",
  "author": "TurtIeSocks <58572875+TurtIeSocks@users.noreply.github.com>",
  "main": "ReactMap.js",
  "workspaces": [
    "apps/**/*",
    "packages/*"
  ],
  "scripts": {
    "build": "vite build",
    "config:check": "yarn workspace @rm/config run check",
    "config:env": "yarn workspace @rm/config run generate",
    "dev": "NODE_ENV=development nodemon server/src/index.js",
    "eslint:check": "eslint \"**/*.{js,jsx}\"",
    "eslint:fix": "eslint \"**/*.{js,jsx}\" --fix",
    "locales:create": "yarn workspace @rm/locales run create",
    "locales:generate": "yarn workspace @rm/locales run generate",
    "locales:missing": "yarn workspace @rm/locales run missing",
    "masterfile": "yarn workspace @rm/masterfile run generate",
    "migrate:latest": "knex --knexfile server/src/db/knexfile.cjs migrate:latest",
    "migrate:make": "knex --knexfile server/src/db/knexfile.cjs migrate:make",
    "migrate:rollback": "knex --knexfile server/src/db/knexfile.cjs migrate:rollback",
    "prettier:check": "prettier --check \"**/*.{css,html,js,jsx,yml}\"",
    "prettier:fix": "prettier --write \"**/*.{css,html,js,jsx,yml}\"",
    "release": "vite build -- -r",
    "server": "node server/src/index.js",
    "start": "node .",
    "watch": "vite"
  },
  "nodemonConfig": {
    "watch": [
      "server/src",
      "packages"
    ],
    "ignore": [
      "server/src/configs/koji_backups/**",
      "server/src/models/queries/**",
      "packages/masterfile/lib/data/*",
      "logs"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.7.15",
    "@apollo/server": "^4.7.4",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@graphql-tools/graphql-file-loader": "^8.0.0",
    "@graphql-tools/load": "^8.0.0",
    "@mui/icons-material": "^5.14.0",
    "@mui/material": "^5.14.0",
    "@rainb0w-clwn/passport-telegram-official": "^2.0.2",
    "@rm/config": "*",
    "@rm/locales": "*",
    "@rm/logger": "*",
    "@rm/masterfile": "*",
    "@sentry/node": "^7.48.0",
    "@sentry/react": "^7.48.0",
    "@turf/boolean-contains": "^6.5.0",
    "@turf/boolean-overlap": "^6.5.0",
    "@turf/boolean-point-in-polygon": "^6.5.0",
    "@turf/center": "^6.3.0",
    "@turf/destination": "^6.5.0",
    "@turf/distance": "^6.5.0",
    "@turf/helpers": "^6.5.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.2",
    "chalkercli": "^1.6.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "discord.js": "^14.7.1",
    "dlv": "^1.1.3",
    "dotenv": "^16.3.1",
    "express": "^4.17.3",
    "express-mysql-session": "^3.0.0",
    "express-rate-limit": "^5.2.6",
    "express-session": "^1.17.3",
    "extend": "^3.0.2",
    "graphql": "^16.6.0",
    "graphql-type-json": "^0.3.2",
    "i18next": "^21.6.16",
    "i18next-browser-languagedetector": "^6.1.4",
    "i18next-fs-backend": "^1.1.4",
    "i18next-http-backend": "^1.4.0",
    "knex": "^2.4.2",
    "leaflet": "1.9.4",
    "leaflet.locatecontrol": "^0.73.0",
    "leaflet.markercluster": "1.5.3",
    "lodash.debounce": "^4.0.8",
    "morgan": "^1.10.0",
    "mysql2": "^3.4.0",
    "node-cache": "^5.1.2",
    "node-fetch": "2",
    "node-geocoder": "^4.2.0",
    "nodes2ts": "^2.0.0",
    "objection": "^3.0.1",
    "ohbem": "^1.4.1",
    "passport": "^0.6.0",
    "passport-discord": "https://github.com/tonestrike/passport-discord.git",
    "passport-local": "^1.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-ga4": "^1.4.1",
    "react-i18next": "^11.16.7",
    "react-leaflet": "4.2.1",
    "react-leaflet-cluster": "2.1.0",
    "react-router-dom": "^6.2.0",
    "react-virtualized-auto-sizer": "^1.0.20",
    "react-virtuoso": "^4.5.0",
    "react-window": "^1.8.9",
    "rtree": "^1.4.2",
    "suncalc": "^1.9.0",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@rm/types": "*",
    "@sentry/vite-plugin": "2.2.1",
    "@types/dlv": "^1.1.2",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "4.0.0",
    "eslint": "^8.44.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "rollup-plugin-delete": "^2.0.0",
    "vite": "4.3.9",
    "vite-plugin-checker": "0.6.0",
    "vite-plugin-static-copy": "0.16.0"
  },
  "engines": {
    "node": ">=18",
    "yarn": "^1.22.x"
  }
}
